{
  "name": "wick-editor-react",
  "version": "0.1.0",
  "private": true,
  "homepage": "./",
  "dependencies": {
    "audiobuffer-to-wav": "^1.0.0",
    "base64-arraybuffer": "^0.2.0",
    "bootstrap": "^4.1.3",
    "classnames": "^2.2.6",
    "file-saver": "^2.0.0",
    "gh-pages": "^2.0.1",
    "localforage": "^1.7.3",
    "node-sass": "^4.9.4",
    "rc-slider": "^8.6.3",
    "react": "^16.5.2",
    "react-ace": "^6.2.0",
    "react-aria-menubutton": "^6.0.1",
    "react-color": "^2.14.1",
    "react-dnd": "^6.0.0",
    "react-dnd-html5-backend": "^6.0.0",
    "react-dom": "^16.5.2",
    "react-dropdown": "^1.6.4",
    "react-dropzone": "^8.0.3",
    "react-ga": "^2.5.6",
    "react-hotkeys": "^2.0.0",
    "react-modal": "^3.8.1",
    "react-numeric-input": "^2.2.3",
    "react-popover": "^0.5.10",
    "react-reflex": "^3.0.6",
    "react-resize-detector": "^3.1.3",
    "react-rnd": "^9.1.1",
    "react-scripts": "2.0.5",
    "react-select": "^2.1.0",
    "react-tabs": "^3.0.0",
    "react-toastify": "^4.5.2",
    "react-tooltip": "^3.9.0",
    "reactstrap": "^6.5.0",
    "underscore": "^1.9.1"
  },
  "main": "src/electron-starter.js",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build && cp CNAME_editor ./build/CNAME",
    "test": "react-scripts test",
    "predeploy": "npm run build",
    "prerelease-deploy": "npm run build; cp -r build/* ../wick-editor-prerelease/; cd ../wick-editor-prerelease; rm CNAME; echo 'test.wickeditor.com' > CNAME; git add .; git commit -m 'update prerelease build'; git push --force;",
    "deploy": "gh-pages -d build",
    "eject": "react-scripts eject",
    "electron-dev-macos": "ELECTRON_START_URL=http://localhost:3000 electron .",
    "electron-dev-windows": "set ELECTRON_START_URL=http://localhost:3000 && electron .",
    "electron": "electron .",
    "electron-build-macos": "electron-packager . wickmacos --ignore=node_modules --platform=darwin --out electron-build",
    "electron-build-windows": "electron-packager . wickwindows --ignore=node_modules --platform=win32 --out electron-build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "electron": "^6.0.10",
    "electron-packager": "^14.0.6"
  }
}
